  "license": "MIT",
  "bugs": {
    "url": "https://github.com/the1812/Touhou-Tagger/issues"
  },
  "homepage": "https://github.com/the1812/Touhou-Tagger#readme",
  "dependencies": {
    "@squoosh/lib": "0.5.3",
    "axios": "^0.27.2",
    "flac-tagger": "^1.0.2",
    "fuse.js": "^6.6.2",
    "glob": "^10.2.7",
    "image-type": "^4.1.0",
    "imageinfo": "^1.0.4",
    "linkedom": "^0.16.4",
    "node-id3": "^0.2.3",
    "ora": "^5.4.1",
    "yargs": "^17.5.1"
  },
  "devDependencies": {
    "@the1812/eslint-config": "1.0.4",
